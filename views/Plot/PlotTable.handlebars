<h1>{{title}}</h1>
<form id="addPlot" action="/plotTable" method="post">
	<table>
		<tbody>
			<tr>
				<td class="formCell"><label for="type">Type: </label></td>
				<td class="formCell">
					<select class="resettableSelector" id="type" name="type">
						<option value="all">*</option>
						<option value="Pasture">Pasture</option>
						<option value="Nightshade">Nightshade</option>
						<option value="Legume">Legume</option>
						<option value="Root Vegetable">Root Vegetable</option>
						<option value="Woodland">Woodland</option>
						<option value="Spring Wheat">Spring Wheat</option>
						<option value="Winter Wheat">Winter Wheat</option>
						<option value="Oats">Oats</option>
						<option value="Legume">Legume</option>
						<option value="Fruit">Fruit</option>
						<option value="N/A">N/A</option>
					</select>
				</td>
				<td class="formCell"></td>
			</tr>
			<tr>
				<td class="formCell"><label for="cycle_status">Cycle Status: </label></td>
				<td class="formCell">
					<select class="resettableSelector" id="cycle_status" name="cycle_status">
						<option value="all">*</option>
						<option value="0">0</option>
						<option value="1">1</option>
						<option value="2">2</option>
						<option value="3">3</option>
						<option value="4">4</option>
						<option value="5">5</option>
					</select>
				</td>
				<td class="formCell"></td>
			</tr>
			<tr>
				<td class="formCell"><label for="previous_type">Previous Type: </label></td>
				<td class="formCell">
					<select class="resettableSelector" id="previous_type" name="previous_type">
						<option value="all">*</option>
						<option value="null">null</option>
						<option value="Pasture">Pasture</option>
						<option value="Nightshade">Nightshade</option>
						<option value="Legume">Legume</option>
						<option value="Root Vegetable">Root Vegetable</option>
						<option value="Woodland">Woodland</option>
						<option value="Spring Wheat">Spring Wheat</option>
						<option value="Winter Wheat">Winter Wheat</option>
						<option value="Oats">Oats</option>
						<option value="Legume">Legume</option>
						<option value="Fruit">Fruit</option>
						<option value="N/A">N/A</option>
					</select>
				</td>
				<td class="formCell"></td>
			</tr>
		</tbody>
	</table>
	<input id="Add Instance" value="Add Instance" type="submit">
	<input type="hidden" id="~filterCriteriaChangedFlag" value="false" name="~filterCriteriaChangedFlag">
	<input id="Filter Table" value="Filter Table" type="submit">
	<span class="labelText"><em>Use the form fields above to add to or filter the table.</em></span>
	<input class= "reset" id="reset" type="submit" name="reset" value="Reset Form">
</form>
<table>
	<thead>
		<th>ID</th>
		<th>Type</th>
		<th>Cycle Status</th>
		<th>Previous Type</th>
	</thead>
	<tbody>
		{{#each plots}}
		<tr>
			<td>{{id}}</td>
			<td>{{type}}</td>
			<td class="alignCellContentCenter">{{cycle_status}}</td>
			<td>{{previous_type}}</td>
			<td class="deleteButton"><button onclick="deleteRow({{id}}, 'plot')">Delete</button></td>
			<td class="updateLink"><a href="/plotTable/{{id}}">Update</a></td>
		</tr>
		{{/each}}
	</tbody>	
</table>
<form action="/plotTable" method="post">
	<button class="homePage" type="submit" value="Home Page" name="Home Page">Home Page</button>
</form>
{{#each jsscriptsTableView}}
	<script src="{{this}}"></script>
{{/each}}
{{#each formFill}}
	<script defer>formFill("{{this.key}}", "{{this.value}}");</script>
	<input class="filterCriteriaCache" type="hidden" id="{{this.key}}FilterCriteriaCache" value="{{this.value}}">
{{/each}}
<script defer>sendParameterAddOrFilter();</script>
<script defer>criteriaChangedStatus();</script>
<script defer>resetForm();</script>